/* empty, add necessary CSS rules below */
h1 {margin: -.8em 0 1.3em; position: relative;}
body, html {font-family: "Myriad Pro",Tahoma,Helvetica,sans-serif; margin: 0; padding: 0; line-height: 160%;}
#hd, .marquee .yui-g, .core {width: 960px; margin: 10px auto; position: relative;}
#bd {padding-left: 1em;}
#logo a img {z-index: 0; position: relative;}
body {background: url('images/body_bg.png') repeat-x 0 113px;}

a {color: green}
a:hover, h1 {color: #0A384F}
a:active, a:focus { outline: none; }

.breadcrumb {display: none}
.marquee img {position: relative; z-index: 0 !important}
.marquee .url {padding:50px 20px; float: right; color: #3f739b}
#block-menu-primary-links {position: absolute; top: 9px; left: 120px; z-index: 10;}
#block-menu-primary-links ul.menu li {display: inline}
#block-menu-primary-links ul.menu ul.menu {display: none}
#block-menu-primary-links ul.menu ul.menu li {display: block; }
#hd {z-index: 3;}
table td {
  border: none;
}

tbody {
  border-top: none;
}
.active, .selected {font-weight: bold; text-decoration: none; color: #555;}
.marquee {background:  url(images/header_bg.png) repeat-x; height:175px; width: 100%; display: block; position: absolute; left: 0; z-index: 0;}
.box {background: url(images/box_bg.png) no-repeat; padding: .5em 2em}
.sbox {padding: 0 1em 2em; clear: left;}
.cbox {padding: 0 0 2em;}

#ft {  border-top: 1px solid #c3e0ef;
  background: #EAF5FA url('images/hd_bg.png') repeat-x bottom;
  clear: both;
  display: block;
  min-height: 100px;
  margin-top: 2em;
padding: 1em;
  }
#ft #block-block-6.block {width: 300px; margin-right: 40px; margin-left: 30px;}
#ft .block {float: left; margin: 0 20px 0 0; width: 175px}
  
.facebook {background: url(images/facebook.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}
.linkedin {background: url(images/linkedin.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}
.youtube {background: url(images/youtube.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}
.flickr {background: url(images/flickr.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}
.rss {background: url(images/feed.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}
.twitter {background: url(images/twitter.png) no-repeat left top; display: block; text-indent: -3000em; height: 60px; width: 60px; float: left;}

.nodots li {margin: 0; list-style: none; position: relative}
.nodots {margin: 0; padding: 0;}

/* Site feed info */
.feed-item-body {height: 4.5em; overflow: hidden;}
#feeds .gfc-result .gf-title, #news1 .gfc-result .gf-title, #news .gfc-result .gf-title {overflow: normal !important; height: auto; font-weight: normal;}
.feed-url, .feed-updated, #news1 .gf-author, #news1 .gf-spacer, #news .gf-author, #news .gf-spacer, #feeds .gf-snippet, #news .gf-snippet {display: none;}

.gfc-tabsArea {margin-left: -5px; margin-bottom: 10px;}
.gfc-tabHeader {font-size: 80%}
.gfc-control {padding: 0}
.gf-relativePublishedDate, .gf-author, .gf-spacer {font-size: 82%; color: #aaa !important;}
.gfc-resultsHeader {display: none;}
.gf-title {font-weight: bold}
.gf-snippet, #news .gfc-result {border-bottom: 1px solid #eee; padding-bottom: .8em}


                .gallery a {
                        outline: none; margin-right: 10px;
                }

                .gallery img {
                        border: 1px solid #CCC;
                        padding: 2px;
                        margin: 10px 5px 10px 0;
position: relative;
                }
.hidden { position:absolute; top:0; left:-9999px; width:1px; height:1px; overflow:hidden; }
/* menu */
li a.active {font-weight: bold; color: #0A384F}
#hd ul.menu li {position: relative; padding: 0; margin: 0; z-index: 10; list-style: none; float: left; border: 1px solid #fff; border-top: none;}
#hd ul.menu li a {width: inherit; display: block; padding: 5px 10px; font-weight: bold;}
#hd ul.menu ul {position: absolute; left: 0; padding: 0; margin: 0 0 0 -1px; display:block; z-index: 10; border: 1px solid #3f739b; border-left: none; border-right: none}
#hd ul.menu ul li {padding: 0; margin: 0; width: 15em; background: #EAF5FA; border: 1px solid #3f739b; border-top: none; border-bottom: 1px solid #c3e0ef}
.hover {background: #EAF5FA url(images/fade_bg.png) repeat-x; margin: 0; z-index: 11; border: 1px solid #3f739b !important;}
.hover .hover {background: #c3e0ef !important; border-top: none !important;border-bottom: 1px solid #c3e0ef !important}
.hover a {text-decoration: none; color: #0A384F}


#fancy_wrap {z-index: 10000}


/* scroll */
.scrollm li {display: none; background-color: transparent !important; float: left; width: 100%; text-align: center; padding: 0; margin: 0 0 1em; list-style: none; vertical-align: top; position: relative;  z-index: 0 !important;}
.scrollm ul {width: 100%; margin: -28px 0 0 0; padding: 0; z-index: 1 !important; position: relative;}
.scrollm {position: relative; z-index: 5;}
.marquee .navx {position: absolute; top: 70px; width: 100%; z-index: 6; clear: both;}
.marquee .nex, .marquee .pre {height: 48px; width: 48px; text-indent: -7000em; position: absolute;}
.marquee .nex {right: 0; background: url('images/arrow_n_b.png') no-repeat; }
.marquee .pre {left: 0; background:  url('images/arrow_p_b.png') no-repeat;}
.marquee .nex:hover {right: 0; background: url('images/arrow_n_b_h.png') no-repeat; }
.marquee .pre:hover {left: 0; background:  url('images/arrow_p_b_h.png') no-repeat;}

/* scroll */
.scrolls li {float: left; width: 100%; text-align: center; padding: 0; margin: 0 0 1em; list-style: none; vertical-align: top; position: relative;  z-index: 0 !important;}
.scrolls ul {width: 100%; margin:0; padding: 0; z-index: 1; position: relative;}
.scrolls {width: 300px;}
.yui-gb .navx {position: absolute; top: 80px; width: 100%; z-index: 3; clear: both;}
.yui-gb .nex, .yui-gb .pre {height: 48px; width: 48px; text-indent: -7000em; position: absolute;}
.yui-gb .nex {right: 0; background: url('images/arrow_n_s.png') no-repeat; }
.yui-gb .pre {left: 0; background:  url('images/arrow_p_s.png') no-repeat;}
.yui-gb .nex:hover {right: 0; background: url('images/arrow_n_s_h.png') no-repeat; }
.yui-gb .pre:hover {left: 0; background:  url('images/arrow_p_s_h.png') no-repeat;}

.tabs {z-index: 2; position: relative;}
.reflected {margin-bottom: 2px;}
.reflect {padding: 2px; border: 1px solid #ddd;}
.site-map p {display: none;}
