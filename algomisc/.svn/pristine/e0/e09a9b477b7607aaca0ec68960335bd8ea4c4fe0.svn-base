//Parse parameter from request url
function gup(name){  
  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");  
  var regexS = "[\\?&]"+name+"=([^&#]*)";  
  var regex = new RegExp( regexS );  
  var results = regex.exec(document.location);  
  if( results == null )    
    return "";  
  else    
    return results[1];
}

	
